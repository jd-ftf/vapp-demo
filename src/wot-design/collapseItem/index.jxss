.wd-collapse-item__header{position:relative;display:flex;justify-content:space-between;align-items:center;padding:11px 15px;overflow:hidden;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}.wd-collapse-item__header:after{position:absolute;display:block;content:"";width:100%;height:1px;left:15px;top:0;transform:scaleY(.5);background:#eee;z-index:1}.wd-collapse-item__title{color:#333;font-size:16px}.wd-collapse-item__arrow{display:block;font-size:14px;color:#c5c5c5;transition:transform .3s}.wd-collapse-item__arrow.is-retract{transform:rotate(-180deg)}.wd-collapse-item__wrapper{position:relative;transition:height .3s ease-in-out;overflow:hidden;will-change:height}.wd-collapse-item__wrapper:after{position:absolute;display:block;content:"";width:100%;height:1px;left:15px;top:0;transform:scaleY(.5);background:#eee;z-index:1}.wd-collapse-item__body{padding:13px 15px;color:#666;font-size:13px}.wd-collapse-item.is-disabled .wd-collapse-item__arrow,.wd-collapse-item.is-disabled .wd-collapse-item__title{color:#d2d2d2}.wd-collapse-item .wd-collapse-item__header-first:after{display:none}