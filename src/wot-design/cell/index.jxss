.wd-cell{padding-left:15px;background-color:#fff;text-decoration:none;color:#333;line-height:1.43;-webkit-tap-highlight-color:transparent}.wd-cell,.wd-cell__wrapper{position:relative;display:flex}.wd-cell__wrapper{flex:1;padding:11px 15px 11px 0;justify-content:space-between}.wd-cell__wrapper:after{position:absolute;display:block;content:"";width:100%;height:1px;left:0;top:0;transform:scaleY(.5);background:#eee;z-index:1}.wd-cell__title{font-size:14px;vertical-align:middle;flex-shrink:0;margin-right:20px}.wd-cell__label{margin-top:3px;font-size:12px;color:#a7a7a7}.wd-cell__right{position:relative;font-size:0;display:flex;flex-direction:row;justify-content:flex-end;flex-wrap:nowrap}.wd-cell__icon{position:relative;min-width:18px;height:42px;line-height:42px;margin-right:15px;font-size:18px}.wd-cell__value{color:#666}.wd-cell__arrow-right,.wd-cell__value{display:inline-block;font-size:14px;vertical-align:middle}.wd-cell__arrow-right{margin-left:5px;height:20px;min-width:14px;line-height:20px;color:#c5c5c5}.wd-cell.is-no-hair .wd-cell__wrapper:after{display:none}.wd-cell.is-link{-webkit-tap-highlight-color:rgba(0,0,0,.06)}.wd-cell.is-hover{background-color:rgba(0,0,0,.06)}.wd-cell.is-large .wd-cell__title{font-size:16px}.wd-cell.is-large .wd-cell__arrow-right{height:22px;line-height:22px}.wd-cell.is-large .wd-cell__icon{height:44px;line-height:44px}.wd-cell.is-center .wd-cell__wrapper{align-items:center}