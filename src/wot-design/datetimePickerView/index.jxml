<wd-picker-view
  custom-class="custom-class"
  id="{{pickerId}}"
  value="{{pickerValue}}"
  columns="{{columns}}"
  cancel-button-text="{{cancelButtonText}}"
  confirm-button-text="{{confirmButtonText}}"
  loading="{{loading}}"
  visible-item-count="{{visibleItemCount}}"
  item-height="{{itemHeight}}"
  bind:change="onChange"
/>